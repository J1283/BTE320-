        "  operator = input('enter an operator: ')\n",
        "  n1=float(input('first number'))\n",
        "  n2=float(input('second number'))\n",
        "  if operator == ('+'):\n",
        "    print(n1+n2)\n",
        "  elif operator == ('-'):\n",
        "    print(n1 - n2)\n",
        "  elif operator == ('*'):\n",
        "    print(n1*n2)\n",
        "  elif operator == ('/'):\n",
        "    print(n1/n2)\n",
        "  elif operator == ('//'):\n",
        "    print(n1//n2)\n",
        "  elif operator == ('**'):\n",
        "    print(n1**n2)\n",
        "  elif operator == ('%'):\n",
        "    print(n1%n2)\n",
        "  elif operator == 'break':\n",
        "    break\n",
        "  else:\n",
        "    print(f'Invalad operator: \\'(operator)\\'')\n",
